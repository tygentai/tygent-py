{
  "name": "tygent-agent-converter",
  "displayName": "Tygent Agent Converter",
  "description": "Convert existing agent implementations to use Tygent with a single command.",
  "version": "0.0.1",
  "publisher": "tygent",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": ["Other"],
  "activationEvents": ["onCommand:tygent.enableAgent"],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "tygent.enableAgent",
        "title": "Tygent: Enable Agent",
        "category": "Tygent"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.75.0",
    "@types/node": "^14.0.0",
    "typescript": "^4.4.0"
  }
}
